# v1/ConfigMap proxy-config
apiVersion: v1
data:
  vhost.docker-registry: |-
    https://registry.www.gitpod-example.com {
        import enable_log
        import remove_server_header
        import ssl_configuration

        basicauth bcrypt "Docker Registry" {
            zWSmfImC1v83kWQiaWqt JDJhJDEwJHZhNXYxZXFSU2Y1dTVqd1dJWHFZVWVOUjBqYXFVV2g4Li4veWRiR3hhZUxheUVFcWJyZHRp
        }

        reverse_proxy https://registry.default.svc.cluster.local {
            flush_interval -1
            transport http {
                tls_trusted_ca_certs /etc/caddy/registry-certs/ca.crt
            }
        }
    }
  vhost.empty: '# Placeholder to avoid errors loading files using a glob pattern'
  vhost.ide-proxy: |-
    https://ide.www.gitpod-example.com {
        import enable_log_debug
        import remove_server_header
        import ssl_configuration

        reverse_proxy  {
            to ide-proxy.default.svc.cluster.local:80
        }
    }
  vhost.kedge: |-
    https://*.www.gitpod-example.com {
        import enable_log
        import remove_server_header
        import ssl_configuration
        import debug_headers

        @kedge header_regexp host Host ^kedge-([a-z0-9]+).www.gitpod-example.com
        handle @kedge {
            reverse_proxy kedge.default.svc.cluster.local:8080 {
                import upstream_headers
                import upstream_connection
            }
        }

        respond "Not found" 404
    }
  vhost.minio: |-
    https://minio.www.gitpod-example.com {
        import enable_log
        import remove_server_header
        import ssl_configuration

        reverse_proxy minio.default.svc.cluster.local:9001 {
            flush_interval -1
        }
    }
  vhost.open-vsx: |-
    https://open-vsx.www.gitpod-example.com {
        import enable_log_debug
        import remove_server_header
        import ssl_configuration

        reverse_proxy  {
            to openvsx-proxy.default.svc.cluster.local:8080
        }
    }
  vhost.payment-endpoint: |-
    https://payment.www.gitpod-example.com {
        import enable_log
        import remove_server_header
        import ssl_configuration
        import debug_headers

        reverse_proxy payment-endpoint.default.svc.cluster.local:3002 {
            import upstream_headers
            import upstream_connection
        }

        handle_errors {
            respond "Internal Server Error" 500
        }
    }
