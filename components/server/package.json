{
  "private": true,
  "name": "@gitpod/server",
  "version": "0.0.0",
  "license": "UNLICENSED",
  "scripts": {
    "start": "node ./dist/src/main.js",
    "start-inspect": "node --inspect=9229 ./dist/src/main.js",
    "start-ee": "node ./dist/ee/src/main.js",
    "start-ee-inspect": "node --inspect=9229 ./dist/ee/src/main.js",
    
    "prepare": "yarn clean && yarn build",
    "build": "npx tsc && ([ -e ee ] && cp ee/src/graphql/*.graphql dist/ee/src/graphql)",
    "build:clean": "yarn clean && yarn build",
    "rebuild": "yarn build:clean",
    "build:watch": "watch 'yarn build' .",
    "watch": "leeway exec --package .:app --transitive-dependencies --filter-type yarn --components --parallel -- tsc -w --preserveWatchOutput",
    "clean": "rimraf dist && rimraf app/lib",
    "clean:node": "rimraf node_modules",
    "purge": "yarn clean && yarn clean:node && yarn run rimraf yarn.lock",
    "test": "mocha --opts mocha.opts 'src/**/*.spec.ts'",
    "test-kube": "mocha --opts mocha.opts '**/*.spec.kube.ts'",
    "db-test-run": "mocha --opts mocha.opts '**/*.spec.db.ts'",
    "telepresence": "leeway run .:telepresence",
    "graphql-codegen": "graphql-codegen --config src/ee/graphql/graphql-codegen.yml"
  },
  "files": [
    "/dist"
  ],
  "dependencies": {
