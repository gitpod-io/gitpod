        "@types/node": "^18.18.8",
        "@types/random-number-csprng": "^1.0.0",
        "@types/uuid": "^8.3.1",
        "@types/ws": "^5.1.2",
        "@typescript-eslint/eslint-plugin": "^5.5.0",
        "@typescript-eslint/parser": "^5.5.0",
