{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "env": {
                "DB_PORT": "23306"
            },
            "name": "Run Mocha Test in Editor",
            "program": "${workspaceFolder}/node_modules/.bin/_mocha",
            "args": [
                "--opts",
                "${workspaceFolder}/mocha.opts",
                "--colors",
                "--timeout",
                "999999",
                "${file}",
                "-g",
                "${selectedText}"
            ],
            "internalConsoleOptions": "openOnSessionStart"
        }
    ],
    "compounds": []
}
